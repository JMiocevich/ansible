#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/tsm@2.3.0/node_modules:/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/tsm@2.3.0/node_modules/tsm/node_modules:/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/tsm@2.3.0/node_modules:/Users/joe/personal/ansible/.config/karabiner/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsm/bin.js" "$@"
else
  exec node  "$basedir/../tsm/bin.js" "$@"
fi
